{
	"name": "barbarian",
	"version": "0.1.0",
	"description": "Barbarian is a Conan meta-server serving package recipes directly from GitHub repositories.",
	"main": "dist/app.js",
	"scripts": {
		"prestart": "npm run build",
		"prebuild": "rm -rf dist/* && b2 src//html && cp .dbconf.mysql.barbarian.bfg.js dist/",
		"build": "tsc",
		"start": "node dist/app.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bfgroup/barbarian.git"
	},
	"keywords": [
		"conan",
		"package-manager",
		"cplusplus",
		"cpp",
		"conan-packages",
		"conan-server"
	],
	"author": "Ren√© Ferdinand Rivera Morell",
	"license": "BSL-1.0",
	"bugs": {
		"url": "https://github.com/bfgroup/barbarian/issues"
	},
	"homepage": "https://github.com/bfgroup/barbarian#readme",
	"dependencies": {
		"express": "^4.17.1",
		"express-universal-analytics": "^0.7.1",
		"mysql": "^2.18.1",
		"node-fetch": "^2.6.1"
	},
	"devDependencies": {
		"@types/express": "^4.17.11",
		"@types/mysql": "^2.15.18",
		"@types/node-fetch": "^2.5.10"
	}
}
