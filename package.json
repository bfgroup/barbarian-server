{
  "name": "barbarian",
  "version": "0.1.0",
  "description": "Barbarian is a Conan meta-server serving package recipes directly from GitHub repositories.",
  "main": "dist/app.js",
  "scripts": {
    "prestart": "npm run build",
    "prebuild": "rm -rf dist/*",
    "build": "tsc && cp -rf package.json dist/package.json",
    "start": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bfgroup/barbarian.git"
  },
  "keywords": [
    "conan",
    "package-manager",
    "cplusplus",
    "cpp",
    "conan-packages",
    "conan-server"
  ],
  "author": "Ren√© Ferdinand Rivera Morell",
  "license": "BSL-1.0",
  "bugs": {
    "url": "https://github.com/bfgroup/barbarian/issues"
  },
  "homepage": "https://github.com/bfgroup/barbarian#readme",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.11"
  }
}
